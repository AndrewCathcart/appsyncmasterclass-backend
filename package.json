{
  "name": "appsyncmasterclass-backend",
  "private": true,
  "version": "1.0.0",
  "description": "Backend for the AppSync Masterclass demo app",
  "main": "serverless.yml",
  "type": "module",
  "scripts": {
    "sls": "sls",
    "exportEnv": "sls export-env",
    "test": "jest --verbose ./src/__tests__/test_cases/unit",
    "test:integration": "sls export-env && jest --verbose ./src/__tests__/test_cases/integration",
    "test:e2e": "sls export-env && jest --verbose ./src/__tests__/test_cases/e2e"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AndrewCathcart/appsyncmasterclass-backend.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/AndrewCathcart/appsyncmasterclass-backend/issues"
  },
  "homepage": "https://github.com/AndrewCathcart/appsyncmasterclass-backend#readme",
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.16.8",
    "@types/jest": "^27.4.0",
    "amplify-appsync-simulator": "^2.3.6",
    "amplify-velocity-template": "^1.4.6",
    "dotenv": "^12.0.1",
    "jest": "^27.4.7",
    "serverless": "^2.71.0",
    "serverless-appsync-plugin": "^1.12.1",
    "serverless-export-env": "^1.4.4",
    "serverless-iam-roles-per-function": "^3.2.0"
  },
  "babel": {
    "env": {
      "test": {
        "plugins": [
          "@babel/plugin-transform-modules-commonjs"
        ]
      }
    }
  },
  "dependencies": {
    "aws-sdk": "^2.1058.0",
    "chance": "^1.1.8"
  }
